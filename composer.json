{
  "name": "voryx/thruway-bundle",
  "type": "symfony-bundle",
  "description": "WebSockets (WAMP2) integration for Symfony2",
  "keywords": [
    "WebSockets",
    "WAMP",
    "WAMP2",
    "Autobahn",
    "Sockets",
    "Symfony"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "David Dan",
      "email": "davidwdan@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Matt Bonneau",
      "email": "matt@bonneau.net",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-0": {
      "Voryx\\ThruwayBundle": "src"
    }
  },
  "require": {
    "php": ">=7.1",
    "symfony/finder": "~2.4|~3.0",
    "voryx/thruway": "~0.5|dev-master",
    "jms/serializer-bundle": "~2.0",
    "react/child-process": "~0.5",
    "thruway/ratchet-transport": "~0.5|dev-master",
    "thruway/pawl-transport": "~0.5|dev-master"
  },
  "require-dev": {
    "symfony/symfony": "^2.8|^3.0",
    "doctrine/orm": "^2.5",
    "symfony/yaml": "2.*|3.*",
    "phpunit/phpunit": "~5.0"
  }
}
